{
  "name": "ahchacha",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native link && react-native run-android",
    "ios": "react-native link && react-native run-ios",
    "web": "razzle start",
    "start": "react-native link && react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "clean:android": "cd android && gradlew clean",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && gradlew assembleDebug",
    "install:android": "cd android/app/build/outputs/apk/debug && adb install app-debug.apk",
    "release:android": "cd android && gradlew bundleRelease"
  },
  "dependencies": {
    "@notifee/react-native": "^3.0.2",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-firebase/app": "^12.9.3",
    "@react-native-firebase/messaging": "^12.9.3",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-native-seoul/kakao-login": "^3.4.0",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@types/react-native-vector-icons": "^6.4.9",
    "axios": "^0.24.0",
    "babel-plugin-inline-import": "^3.0.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.2",
    "react-native": "0.66.1",
    "react-native-bootsplash": "^3.2.6",
    "react-native-calendars": "^1.1268.0",
    "react-native-date-picker": "^4.1.1",
    "react-native-daum-postcode": "^1.0.11",
    "react-native-dropdown-picker": "^5.2.3",
    "react-native-geolocation-service": "^5.3.0-beta.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-maps": "^0.29.3",
    "react-native-paper": "^4.9.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^9.0.0",
    "react-native-webview": "^11.14.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
